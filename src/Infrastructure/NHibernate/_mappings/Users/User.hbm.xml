<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ProcentCqrs.Domain.Users"
                   namespace="ProcentCqrs.Domain.Users"
                   >
    <class name="User" table="Users">
        <id type="int" name="_id" access="field" unsaved-value="0">
            <column name="Id" />
            <generator class="identity"></generator>
        </id>
        <property name="_email" column="Email" access="field"/>
        <property name="_firstName" column="FirstName" access="field"/>
        <property name="_lastName" column="LastName" access="field"/>
    </class>
</hibernate-mapping>